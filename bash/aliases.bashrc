# commands
alias lsa='ls -a'
alias lsA='ls -all'
alias cd..='cd ..'
alias cls='clear'
alias clsa='clear && lsa'
alias clsA='clear && lsA'

# git
alias gits='git status'
alias gitlg='git log --oneline'
alias gitlog='git log'
alias gitlogg='git log --stat'
alias gitbr='git branch'
