export TERMINAL=alacritty
export EDITOR=nvim

alias v=$EDITOR

# commands
alias cd..='cd ..'
alias cls='clear'
alias clsa='clear && ls -a'
alias clsA='clear && ls -all'
alias lsa='ls -a'
alias lsA='ls -all'
